<!DOCTYPE html>
<html>
<head>
	<!-- Meta -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Responsive -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

     <!-- Titulo -->
    <title>Login | Auto Servicio</title>

    <!-- Roboto -->
    <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>

    <!-- Favicon -->
    <link rel="icon" href="styles/images/favicon.png" type="images/x-icon">

    <!-- Signup.css Custom -->
    <link rel="stylesheet" href="/styles/signup.css" >

    <!-- Material Design -->
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-light_blue.min.css">

    <!-- Google Icons -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>
<body>

	    <!-- Barra de navegación -->
    <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
      <header class="mdl-layout__header">
        <div class="mdl-layout__header-row">
          <!-- Title -->
          <span class="mdl-layout-title">Autoservicios 2017</span>
          <!-- Add spacer, to align navigation to the right -->
          <div class="mdl-layout-spacer"></div>
          <!-- Navigation. We hide it in small screens. -->
        </div>
      </header>
      <div class="mdl-layout__drawer">
        <span class="mdl-layout-title">MENU</span>
        <nav class="mdl-navigation">
          <a class="mdl-navigation__link" href="/">Home</a>
        </nav>
      </div>
      <main class="mdl-layout__content">
        <div class="page-content">
        <!-- Content -->
        <div class="demo-layout mdl-layout mdl-layout--fixed-header mdl-js-layout mdl-color--grey-100">
          <div class="demo-ribbon mdl-color--teal"></div>
          <main class="demo-main mdl-layout__content">
            <h2 align="center" class=" mdl-color-text--white text-shadow">Auto Servicios 2017</h2>
            <a id="top"></a>
            <div class="demo-container mdl-grid">
              <div class="mdl-cell mdl-cell--4-col mdl-cell--hide-tablet mdl-cell--hide-phone"></div>
              <div class="demo-content mdl-color--white mdl-shadow--4dp content mdl-color-text--grey-800 mdl-cell mdl-cell--4-col mdl-cell--12-col-tablet">

                <div class="mdl-card__title ">
                  <h2 class="mdl-card__title-text">
                    <i class="material-icons mdl-color-text--grey m-r-5 lh-13">account_circle</i>
                    &nbsp;Entrar
                  </h2>
                </div>
                <div class="p-l-20 p-r-20 p-b-20">
                  <form method="POST" action="/login-incio-cliente">
                    <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label textfield-demo">
                      <input class="mdl-textfield__input" type="email" name="email" />
                      <label class="mdl-textfield__label" for="email">Correo</label>
                    </div>
                    <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label textfield-demo">
                      <input class="mdl-textfield__input" type="password" name="clave" />
                      <label class="mdl-textfield__label" for="contraseña">Contraseña</label>
                    </div>

                    <div class="m-t-20">
                    <button type="submit" class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored mdl-js-ripple-effect mdl-color--light-blue">
                      Entrar
                    </button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </main>
        </div>

        </div>
      </main>
    </div>

    <!-- Jquery -->
    <script src="/js/dependencies/jquery.js"></script>

    <!-- Material Design Lite -->
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>

</body>
</html>
